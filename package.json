{
  "name": "2dispute",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,css,md,html,json,graphql}\"",
    "format:run": "prettier --write \"**/*.{js,jsx,ts,tsx,css,md,html,json,graphql}\"",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "autoprefixer": "^9.7.6",
    "next": "9.3.4",
    "postcss-import": "^12.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-twitter-embed": "^3.0.3",
    "tailwindcss": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "prettier": "^2.0.4",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md,html,json,graphql}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  }
}
